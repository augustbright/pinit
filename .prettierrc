{
	"singleQuote": true,
	"tabWidth": 4,
	"useTabs": false,
	"arrowParens": "avoid",
	"bracketSpacing": true,
	"insertPragma": false,
	"requirePragma": false,
	"trailingComma": "all",
	"semi": true,
	"printWidth": 100,
	"importOrder": [
		"^[./]"
	],
	"importGroupOrder": "length",
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}